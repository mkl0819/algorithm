package com.ssafy.d3;

import java.io.BufferedReader;
import java.io.FileInputStream;
import java.io.InputStreamReader;
import java.util.HashSet;

public class Sollution_재경이의오타 {
	static int T, N;
	static char[] alpha, typo;
	static HashSet<String> words;

	public static void main(String[] args) throws Exception {
		System.setIn(new FileInputStream("res/Solution_재경이의오타.txt"));
		BufferedReader in = new BufferedReader(new InputStreamReader(System.in));

		T = Integer.parseInt(in.readLine().trim());
		for (int test_case = 1; test_case <= T; test_case++) {
			alpha = in.readLine().toCharArray();
			N = alpha.length;
			typo = new char[N];
			combi(0, 0);
		}
	}

	private static void combi(int count, int start) {
		if (count == N) {
			System.out.println(typo.toString());
			return;
		}
		for (int i = -1; i < 2; i++) {
			if ((count == 0 && i == -1) || (count == N - 1 && i == 2)) {
				continue;
			}
			typo[count] = alpha[count + i];
			combi(count + 1, start);
		}

	}
}
